body
  font-family $family-primary
  letter-spacing .6px
  margin 0
  background $grey-100-color 
  text-rendering optimizeLegibility
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  color $default-color 

h1
  font-size 3rem

h2
  font-size 2rem

.common-page-title
.common-intro-text
  color $white-color

.clearBank-background
  background-image linear-gradient(-81deg,$secondary-color 1%,$primary-color)
  transform: skewY(-6deg)
  position absolute
  top -130px
  left 0
  width 100%
  height 600px

.sr-only
  position absolute !important
  width 1px !important
  height 1px !important
  padding 0 !important
  margin -1px !important
  overflow hidden !important
  clip rect(0,0,0,0) !important
  border 0 !important
  
.bold
  font-weight 900  